        <ownedChild>
          <domainMetaElement
              href="components.ecore#//EventBPort"/>
          <domainSpecialization
              body="self.causality = VariableCausality::Output"/>
          <domainInitializer
              xsi:type="gmfmap:FeatureSeqInitializer">
            <initializers
                xsi:type="gmfmap:FeatureValueSpec">
              <feature
                  xsi:type="ecore:EAttribute"
                  href="components.ecore#//AbstractVariable/causality"/>
              <value
                  body="VariableCausality::Output"/>
            </initializers>
          </domainInitializer>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              readOnly="true"
              viewPattern="{0} : {1}"
              editorPattern=""
              editPattern="">
            <diagramLabel
                href="components.gmfgraph#PortLabel"/>
            <features
                href="components.ecore#//NamedElement/name"/>
            <features
                href="components.ecore#//AbstractVariable/type"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.4"/>
          <diagramNode
              href="components.gmfgraph#OutputPort"/>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.1/@ownedChild/@compartments.0">
        <containmentFeature
            href="components.ecore#//EventBComponent/variables"/>
        <ownedChild>
          <domainMetaElement
              href="components.ecore#//EventBVariable"/>
          <labelMappings
              xsi:type="gmfmap:ExpressionLabelMapping"
              readOnly="true">
            <diagramLabel
                href="components.gmfgraph#VariableLabel"/>
            <viewExpression
                body="return &quot;&lt;variable>&quot;;"
                language="java"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="components.gmfgraph#VariableLabel"/>
        </ownedChild>
      </children>
      <compartments
