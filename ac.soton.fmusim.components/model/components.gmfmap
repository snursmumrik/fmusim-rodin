            </initializers>
          </domainInitializer>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              readOnly="true"
              viewPattern="{0} : {1}">
            <diagramLabel
                href="components.gmfgraph#PortLabel"/>
            <features
                href="components.ecore#//NamedElement/name"/>
            <features
                href="components.ecore#//AbstractVariable/type"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="components.gmfgraph#InputPort"/>
        </ownedChild>
      </children>
      <children>
        <containmentFeature
