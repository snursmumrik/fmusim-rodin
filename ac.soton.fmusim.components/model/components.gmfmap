  <links>
    <tool
        xsi:type="gmftool:CreationTool"
        href="components.gmftool#//@palette/@tools.0/@tools.6"/>
    <diagramLink
        href="components.gmfgraph#Link"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="components.ecore#//Port/connector"/>
    <creationConstraints>
      <targetEnd
          body="not (oppositeEnd.oclAsType(AbstractVariable).causality = VariableCausality::Output and self.oclAsType(Connector).ports->exists(p | p.oclAsType(AbstractVariable).causality = VariableCausality::Output))"/>
    </creationConstraints>
  </links>
  <diagram>
    <diagramCanvas
        href="components.gmfgraph#components"/>
    <domainModel
        href="components.ecore#/"/>
    <domainMetaElement
        href="components.ecore#//ComponentDiagram"/>
    <palette
        href="components.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
