  <nodes>
    <containmentFeature
        href="components.ecore#//ComponentDiagram/components"/>
    <ownedChild>
      <domainMetaElement
          href="components.ecore#//FMUComponent"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="components.gmfgraph#FMUComponentName"/>
        <features
            href="components.ecore#//NamedElement/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="components.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="components.gmfgraph#FMUComponent"/>
      <children>
        <containmentFeature
            href="components.ecore#//Component/inputs"/>
        <ownedChild>
          <domainMetaElement
              href="components.ecore#//FMUPort"/>
          <domainSpecialization
              body="self.causality = VariableCausality::Input"/>
          <domainInitializer
              xsi:type="gmfmap:FeatureSeqInitializer">
            <initializers
                xsi:type="gmfmap:FeatureValueSpec">
              <feature
                  xsi:type="ecore:EAttribute"
                  href="components.ecore#//AbstractVariable/causality"/>
              <value
                  body="VariableCausality::Input"/>
            </initializers>
          </domainInitializer>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              readOnly="true"
              viewPattern="{0} : {1}">
            <diagramLabel
                href="components.gmfgraph#PortLabel"/>
            <features
                href="components.ecore#//NamedElement/name"/>
            <features
                href="components.ecore#//AbstractVariable/type"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="components.gmfgraph#InputPort"/>
        </ownedChild>
      </children>
      <children>
        <containmentFeature
            href="components.ecore#//Component/outputs"/>
        <ownedChild>
          <domainMetaElement
              href="components.ecore#//FMUPort"/>
          <domainSpecialization
              body="self.causality = VariableCausality::Output"/>
