          </domainInitializer>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              readOnly="true"
              viewPattern="{0} : {1}">
            <diagramLabel
                href="components.gmfgraph#PortLabel"/>
            <features
                href="components.ecore#//NamedElement/name"/>
            <features
                href="components.ecore#//AbstractVariable/type"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.4"/>
          <diagramNode
              href="components.gmfgraph#OutputPort"/>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="components.ecore#//FMUComponent/variables"/>
        <ownedChild>
          <domainMetaElement
              href="components.ecore#//FMUVariable"/>
          <labelMappings
              xsi:type="gmfmap:ExpressionLabelMapping"
              readOnly="true">
            <diagramLabel
                href="components.gmfgraph#VariableLabel"/>
            <viewExpression
                body="return &quot;&lt;variable>&quot;;"
                language="java"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="components.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="components.gmfgraph#VariableLabel"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.2">
        <compartment
            href="components.gmfgraph#FMUVariablesCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <nodes>
